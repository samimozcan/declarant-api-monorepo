datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL_SEC")
}

generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/sec-client"
}

model SecNctsPackage {
  id                  String  @id @map("kapid") @db.UniqueIdentifier
  itemId              String  @map("kalemid") @db.UniqueIdentifier
  marksOfPackages     String? @map("markanumara")
  marksOfPackagesLang String? @map("dil")
  kindOfPackages      String? @map("kaptip")
  numberOfPackages    Int?    @map("kapadet")

  @@map("ncts_kap")
}

model SecNctsLog {
  id              String  @id @map("logid") @db.UniqueIdentifier
  nctsDocumentId  String  @map("beyannameid") @db.UniqueIdentifier
  recordId        String? @map("kayitid")
  messagetype     String? @map("messagetype")
  customsUserCode String? @map("bilgeuser")
  xmlRequest      String? @map("xmlrequest")
  xmlResponse     String? @map("xmlresponse")
  result          String? @map("sonuc")

  @@map("ncts_log")
}

